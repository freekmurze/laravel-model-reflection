{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [{
        "type": "path",
        "url": "./packages/*",
        "options": {
            "symlink": true
        }
    }
    ],
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "beyondcode/laravel-websockets": "^1.13.2",
        "cknow/laravel-money": "^7.0",
        "hammerstone/fast-paginate": "^0.1.11",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^9.44",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.6",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^0.27",
        "nwidart/laravel-modules": "^9.0",
        "pusher/pusher-php-server": "^7.0.2",
        "spatie/laravel-activitylog": "^4.6",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-model-states": "^2.4",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-tags": "^4.3",
        "spatie/laravel-translatable": "^6.0",
        "spatie/laravel-translation-loader": "^2.7",
        "team-nifty-gmbh/tall-datatables": "dev-main",
        "wireui/heroicons": "^2.3",
        "wireui/wireui": "^1.12"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.6",
        "brianium/paratest": "^6.6",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^v3.6",
        "laravel/pint": "^1.1",
        "laravel/sail": "^1.0.1",
        "laravel/tinker": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "files": [
            "app/helpers.php",
            "app/frontend-helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
